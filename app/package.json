{
  "name": "fokus-zeit",
  "productName": "FokusZeit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite --host\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development npx electron .\"",
    "electron:build": "vite build && electron-builder",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,vue}\""
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.7",
    "@tailwindcss/vite": "^4.1.7",
    "autoprefixer": "^10.4.21",
    "axios": "^1.9.0",
    "better-sqlite3": "^11.10.0",
    "dayjs": "^1.11.13",
    "electron-log": "^5.4.0",
    "electron-store": "^10.0.1",
    "uuid": "^11.1.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^36.3.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^15.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.7",
    "vite": "^6.3.5",
    "vitest": "^3.1.4",
    "wait-on": "^7.2.0"
  }
}
